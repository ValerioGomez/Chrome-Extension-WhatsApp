{
    "name": "Enviar Mensaje WhatsApp",
    "description": "Envie mensajes WhatsApp Web ingresando el numero. ",
    "version": "1.0.1",
    "manifest_version": 3,
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon":{
            "16":"logo16.png",
            "32":"logo32.png",
            "48":"logo48.png",
            "128":"logo128.png"
        }
    }
}